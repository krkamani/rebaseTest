	g_form.hideFieldMsg('receipients', true);
	var receipients = g_form.getValue('receipients').trim();
	var recipientCCErrorEmails = [];
	var recipientCCEmails = [];
	if (receipients &amp;&amp; (receipients.indexOf(',') &gt; 0 || receipients.indexOf(';') &gt; 0)) {
			recipientCCEmails = receipients.split(';').join(',').split(',');
			if (recipientCCEmails &amp;&amp; recipientCCEmails.length &gt; 0) {
				for (var i=0;i&lt;recipientCCEmails.length;i++) {
					if (!isValidEmail(recipientCCEmails[i])) {
						recipientCCErrorEmails.push(recipientCCEmails[i]);
					}
				}	
			}
			g_form.hideFieldMsg('receipients', true);
			if (recipientCCErrorEmails &amp;&amp; recipientCCErrorEmails.length &gt; 0) {
				g_form.showFieldMsg('receipients', recipientCCErrorEmails.toString() + (recipientCCErrorEmails.length &gt; 1 ? " are" : " is") + " invalid " 
									+ (recipientCCErrorEmails.length &gt; 1 ? "  emails" : " email") +  " please re-enter in correct format.", 'error');
				return false;
			}
		} else if (receipients) {
			if (!isValidEmail(receipients)) {
				g_form.showFieldMsg('receipients', receipients + " is an invalid email, please re-enter email in correct format.", 'error');
				return false;
			}
		}
	
	function isValidEmail(email){
		if (!(/^(\w+\s)*\w+\s?&lt;\w+([.-]?\w+)*@\w+([.-]?\w+)*(\.\w{2,}&gt;)+$/.test(email.trim())) &amp;&amp; !(/^\w+([.-]?\w+)*@\w+([.-]?\w+)*(\.\w{2,})+$/.test(email.trim())))
			return false; 
		else
			return true;
	}
   
